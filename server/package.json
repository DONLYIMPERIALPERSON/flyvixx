{
  "name": "flyvixx-server",
  "version": "1.0.0",
  "description": "Backend server for Flyvixx crash game",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && cp -r src/templates dist/",
    "start": "node dist/index.js",
    "add-admin": "ts-node scripts/add-admin.ts",
    "daily-gift-reset": "ts-node scripts/daily-gift-reset.ts",
    "simulate-deposit": "ts-node scripts/simulate-deposit.ts",
    "test-daily-gifts": "ts-node scripts/test-daily-gift-system.ts",
    "simulate-day-2": "ts-node scripts/simulate-day-2.ts",
    "simulate-30-day-lock": "ts-node scripts/simulate-30-day-lock.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "crash-game",
    "websocket",
    "real-time",
    "gaming"
  ],
  "author": "Flyvixx Team",
  "license": "MIT",
  "dependencies": {
    "@descope/node-sdk": "^1.9.1",
    "@types/ioredis": "^4.28.10",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^8.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
